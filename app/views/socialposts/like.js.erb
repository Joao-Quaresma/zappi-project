$("#like_<%= @socialpost.id %>").removeClass('glyphicon-heart-empty').addClass('glyphicon-heart');
$("#like_<%= @socialpost.id %>").removeAttr('href').attr('href', '/socialposts/<%= @socialpost.id %>/unlike');
$("#likes_<%= @socialpost.id %>").html("<%= j (render partial: 'socialposts/likes', locals: { socialpost: @socialpost} ) %>");