<div class="row">
  <div id="notifications" class="results-blocks col-md-4 col-md-offset-4 col-xs-8 col-xs-offset-2">
    <div class="panel panel-info notifications-table">
      <!-- Default panel contents -->
      <div class="panel-heading">
        <div align="center">
          <h1>Social Notifications</h2>
        </div>
      </div>
      <div class="panel-body">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Status</th>
              <th>Time ago</th>
              <th>Info</th>
            </tr>
          </thead>
          <%= render @notifications.where(read: false), selector: "long" %>
        </table>
      </div>
    </div>
  </div>
</div>