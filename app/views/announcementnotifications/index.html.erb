<div class="row">
  <div id="announcementnotifications" class="results-blocks col-xs-12 col-md-10 col-md-offset-1">
    <div class="panel panel-info notifications-table">
      <!-- Default panel contents -->
      <div class="panel-heading">
        <div align="center">
          <h1>Announcement Notifications</h2>
        </div>
      </div>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Status</th>
            <th>Time ago</th>
            <th>Info</th>
            <th>Title</th>
          </tr>
        </thead>
        <%= render @announcementnotifications.where(read: false), selector: "long" %>
      </table
    </div>
  </div>
</div>
